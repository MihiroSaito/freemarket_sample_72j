.completePage
  .completePage__header
    .completePage__header__logo
      = link_to root_path do
        = image_tag "logo.png", class: "login_logo"
    .completePage__header__progress
      %ul
        %li
          会員情報
        %li
          電話番号認証
        %li.active
          完了
      .completePage__header__progress__bars
        .completePage__header__progress__bars__1
          .completePage__header__progress__bars__1__circle1
          .completePage__header__progress__bars__1__circle2
        .completePage__header__progress__bars__2
          .completePage__header__progress__bars__2__circle3
  .completePage__main
    .completePage__main__contents
      %h2 登録が完了しました
      .completePage__main__contents__box
        %h3 ご登録ありがとうございます。会員情報の登録が完了致しました。
        =link_to "トップページへ", root_path, class: "toNextBtn"

  .completePage__footer
    .completePage__footer__inner
    %nav.completePage__footer__inner__nav
      %ul.completePage__footer__inner__nav__category
        %li
          = link_to "プライバシーポリシー", "#", class: "privacyPolicy"
        %li
          = link_to "フリマ利用規約", "#", class: "terms"
        %li
          = link_to "特定商取引に関する表記", "#", class: "notation"
      .completePage__footer__inner__nav__logo
        = link_to root_path do
          = image_tag "logo-white.png"
        %p &copy Furima, Inc.